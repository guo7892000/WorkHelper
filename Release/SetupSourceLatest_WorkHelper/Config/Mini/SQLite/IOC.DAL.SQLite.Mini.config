    <component id="IDMiniDBInitializer" service="Breezee.Framework.Mini.IDAL.IDMiniDBInitializer, Breezee.Framework.Mini.IDAL"
               type="Breezee.Framework.Mini.DAL.SQLite.DMiniDBInitializer,  Breezee.Framework.Mini.DAL.SQLite" />
